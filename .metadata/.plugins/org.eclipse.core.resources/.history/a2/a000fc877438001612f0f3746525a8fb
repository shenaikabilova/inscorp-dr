package inscorp;

import java.util.Date;

import org.junit.AfterClass;
import org.junit.Assert;
import org.junit.Test;

import dao_api.KaskoDAO;
import dao_jdbc.KaskoDAOImpl;
import model.Kasko;

/**
 * @author shenaikabilova
 *
 */
public class KaskoDAOImplTest {
	@SuppressWarnings("deprecation")
	private static Kasko kasko = new Kasko("000001", "000014", "Физическо лице", "Иван", "Иванов", "Иванов", 
			"8807070011", "123456", "гр. Варна", 9000, "В4589АА", "Автомобил", "А14789АRD78954123", 
			"Ауди", "А5", "Бял", 2005, new Date(2005, 10, 10), 2.0f, 4, 10000, 500);
	private static KaskoDAO dao = new KaskoDAOImpl();
	
	@AfterClass
	public static void del() {
		dao.deleteKasko(kasko.getInsKaskoId());
	}
	
	@Test
	public void testListAllKasko(){
		Assert.assertFalse(dao.listKasko().isEmpty());
	}
	
	@Test
	public void insertKasko () {
		dao.insertKasko(kasko);
		
		Assert.assertEquals(kasko.getInsurerId(), dao.searchKasko("000014").getInsurerId());
		Assert.assertEquals(kasko.getInsKaskoId(), dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getInsType(), dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getInsurenceFirstName(), dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getInsurenceSecondName(), dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getInsurenceFamily(), dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getinsurenceEGN, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getinsurenceMobilePhone, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getinsurenceAddress, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getinsurencePK, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleRegistrationNumber, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleType, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleRama, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleBrand, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleModel, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleColor, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleYear, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleFirstRegistration, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getfloat vehicleEngine, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehiclePlaceNumber, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleInsurenceValue, dao.searchKasko("000014"));
		Assert.assertEquals(kasko.getvehicleInsurencePremiq, dao.searchKasko("000014"));
	}
	
