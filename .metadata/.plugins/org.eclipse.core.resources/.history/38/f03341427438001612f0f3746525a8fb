		Assert.assertFalse(dao.listKasko().isEmpty());
	}
	
	@Test
	public void insertKasko () {
		dao.insertKasko(kasko);
		
		Assert.assertEquals(kasko.getInsurerId(), dao.searchKasko("000014")
		Assert.assertEquals(kasko.getinsKaskoId;
		Assert.assertEquals(kasko.getinsType;
		Assert.assertEquals(kasko.getinsurenceFirstName;
		Assert.assertEquals(kasko.getinsurenceSecondName;
		Assert.assertEquals(kasko.getinsurenceFamily;
		Assert.assertEquals(kasko.getinsurenceEGN;
		Assert.assertEquals(kasko.getinsurenceMobilePhone;
		Assert.assertEquals(kasko.getinsurenceAddress;
		Assert.assertEquals(kasko.getinsurencePK;
		Assert.assertEquals(kasko.getvehicleRegistrationNumber;
		Assert.assertEquals(kasko.getvehicleType;
		Assert.assertEquals(kasko.getvehicleRama;
		Assert.assertEquals(kasko.getvehicleBrand;
		Assert.assertEquals(kasko.getvehicleModel;
		Assert.assertEquals(kasko.getvehicleColor;
		Assert.assertEquals(kasko.getvehicleYear;
		Assert.assertEquals(kasko.getvehicleFirstRegistration;
		Assert.assertEquals(kasko.getfloat vehicleEngine;
		Assert.assertEquals(kasko.getvehiclePlaceNumber;
		Assert.assertEquals(kasko.getvehicleInsurenceValue;
		Assert.assertEquals(kasko.getvehicleInsurencePremiq;
	}
	
