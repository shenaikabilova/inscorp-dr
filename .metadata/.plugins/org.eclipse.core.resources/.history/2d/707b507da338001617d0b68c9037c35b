		Assert.assertEquals(go.getinsurenceMobilePhone;
		Assert.assertEquals(go.getvehicleRegNumber;
		Assert.assertEquals(go.getzone;
		Assert.assertEquals(go.getvehicleType;
		Assert.assertEquals(go.getvehicleRama;
		Assert.assertEquals(go.getvehicleBrand;
