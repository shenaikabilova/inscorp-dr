		String id = "000005";
		int countZeros=0;
		int max=0;
		for(int i=0; i<id.length(); i++) {
			char c = id.charAt(i);
			System.out.println(c);
			if(c == '0') {
				countZeros++;
				System.out.print(countZeros + " ");
				max=countZeros;
			}
			else countZeros=0;
		}
		int i = Integer.parseInt(id);
		i++;
		String str = String.format("%0"+max+"d", i); 
		System.out.println(str);
	}
