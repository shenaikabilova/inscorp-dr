function change() {
	var insValue = document.getElementById("vehicleInsValue").value;
//	document.getElementById('vehicleInsPremiq').value=insValue*0.05;
	
	document.getElementById("vehicleInsPremiq").value = "";
	
	switch(document.getElementById("vehicleType")) {
	case 'Автомобил':
		document.getElementById('vehicleInsPremiq').value=insValue*0.05;
	break;
	
	case 'Товарни автомобили':
		
	break;
	
	case 'Мотоциклет':
		document.getElementById('vehicleInsPremiq').value=insValue*0.053;
	break;
		
	case 'Автобус':
		document.getElementById('vehicleInsPremiq').value=insValue*0.03;
	break;

	}
}